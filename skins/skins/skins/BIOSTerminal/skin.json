{
	"name": "BIOSTerminal",
	"version": "1.0.0",
	"author": [
		"Your Name"
	],
	"url": "https://github.com/your-repo/BIOSTerminal",
	"descriptionmsg": "biosterminal-skin-desc",
	"namemsg": "skinname-biosterminal",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"ValidSkinNames": {
		"biosterminal": {
			"class": "SkinBIOSTerminal",
			"args": [
				{
					"name": "biosterminal",
					"templateDirectory": "templates/",
					"styles": [
						"skins.biosterminal"
					],
					"scripts": [
						"skins.biosterminal"
					],
					"template": "BIOSTerminalTemplate"
				}
			]
		}
	},
	"MessagesDirs": {
		"BIOSTerminal": [
			"i18n"
		]
	},
	"ResourceModules": {
		"skins.biosterminal.fonts": {
			"styles": {
				"resources/fonts/fonts.css": {
					"media": "all"
				}
			}
		},
		"skins.biosterminal": {
			"class": "MediaWiki\\ResourceLoader\\SkinModule",
			"features": [
				"elements",
				"content",
				"interface",
				"logo",
				"legacy"
			],
			"targets": [
				"desktop",
				"mobile"
			],
			"styles": {
				"resources/css/BIOSTerminal.css": {
					"media": "all"
				}
			},
			"scripts": [
				"resources/js/biosterminal.js"
			],
			"dependencies": [
				"mediawiki.util",
				"skins.biosterminal.fonts"
			]
		},
		"skins.biosterminal.white-black": {
			"styles": {
				"resources/css/BIOSTerminal-white-black.css": {
					"media": "all"
				}
			},
			"dependencies": [
				"skins.biosterminal"
			]
		},
		"skins.biosterminal.black-white": {
			"styles": {
				"resources/css/BIOSTerminal-black-white.css": {
					"media": "all"
				}
			},
			"dependencies": [
				"skins.biosterminal"
			]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "",
		"remoteSkinPath": "BIOSTerminal"
	},
	"AutoloadClasses": {
		"SkinBIOSTerminal": "includes/SkinBIOSTerminal.php",
		"BIOSTerminalTemplate": "includes/BIOSTerminalTemplate.php"
	},
	"manifest_version": 2,
	"Hooks": {
		"GetPreferences": "SkinBIOSTerminal::onGetPreferences"
	}
}